{
  "name": "fast-trading-api",
  "type": "module",
  "license": "MIT",
  "version": "1.10.1",
  "exports": {
    ".": "./dist/index.js",
    "./utils": "./dist/utils.js",
    "./types": "./dist/types.js"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "start": "bun run dist/index.js",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "test": "bun test",
    "dev": "bun run --watch src/index.js",
    "lint": "bun run lint:eslint && bun run lint:tsc",
    "lint:eslint": "eslint src",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@types/bun": "latest",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "tsc-alias": "^1.8.16",
    "typescript-eslint": "^8.56.0",
    "viem": "^2.46.2"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@iam4x/reconnecting-websocket": "^1.4.0",
    "@msgpack/msgpack": "^3.1.3",
    "@noble/curves": "^1.9.7",
    "@noble/hashes": "^1.8.0"
  }
}
